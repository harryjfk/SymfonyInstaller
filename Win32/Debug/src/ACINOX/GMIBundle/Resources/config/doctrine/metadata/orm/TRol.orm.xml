<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TRol" table="t_rol">
    <id name="idrol" type="integer" column="idrol">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="namerol" type="string" column="namerol" length="50" nullable="false"/>
    <field name="typerol" type="integer" column="typerol" nullable="false"/>
    <field name="descrol" type="text" column="descrol" nullable="false"/>
    <field name="isanon" type="integer" column="isanon" nullable="false"/>
  </entity>
</doctrine-mapping>
